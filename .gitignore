#
# NOTE! Don't add files that are generated in specific
# subdirectories here. Add them in the ".gitignore" file
# in that subdirectory instead.
#
# Normal rules
#
.*
*.o
*.o.*
*.a
*.s
*.su
*.mod.c
*.i
*.lst
*.order
*.elf
*.swp
*.bin
*.patch
*.cfgtmp
*.log
*.txt

# host programs on Cygwin
*.exe

# lzma host util
host_util/lzma/lzma

#
# Top-level generic files
#
/uboot/System.map
/uboot/u-boot*
/uboot/bootstrap
/uboot/bootstrap.map

# bin folder
/bin/

# Symbolic links and some compilation results from tools
/uboot/tools/crc32.c
/uboot/tools/environment.c
/uboot/tools/envcrc
/uboot/tools/mkimage
/uboot/httpd/fsdata.c

# Ignore HTML files in pig directory
/uboot/httpd/vendors/pig/*.html

# Generated include files
/uboot/include/asm
/uboot/include/config.h
/uboot/include/config.mk
/uboot/include/version_autogenerated.h
# TODO: broken symbolic link
/uboot/include/asm-*/arch

# Do not ignore original u-boot images
!/original_u-boot_images/*.bin

#
# git files that we don't want to ignore even it they are dot-files
#
!.gitignore
!.mailmap

# stgit generated dirs
patches-*
.stgit-edit.txt

# quilt's files
patches
series

# gdb files
.gdb_history

# cscope files
cscope.*

# tags files
/tags
/ctags
/etags

# gnu global files
GPATH
GRTAGS
GSYMS
GTAGS

*.orig
*~
\#*#
